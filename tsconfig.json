{
  "compilerOptions": {
    "lib": ["es2019"],
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "target": "es2019",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
    "include": ["nodes/**/*.ts"],
    "exclude": [
      "node_modules", 
      "**/*.spec.ts"
  ]
  }
  